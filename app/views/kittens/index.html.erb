<h1>All Kittens</h1>

<ul id="kitten">
<% @kittens.each do |kitten|%>
<li>
<%= link_to kitten.name, kitten_path(kitten), class: 'kitten_name'%>
<%= link_to "delete", kitten_path(kitten), method: :delete, data: {confirm: "Really delete the kitten?"} %>
<%= link_to "edit", edit_kitten_path(kitten) %>
</li>
<% end %>
</ul>